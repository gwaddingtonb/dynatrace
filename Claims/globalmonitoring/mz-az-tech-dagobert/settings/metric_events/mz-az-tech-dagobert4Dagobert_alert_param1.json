{
    "schemaId": "builtin:anomaly-detection.metric-events",
    "schemaVersion": "1.0.14",
    "scope": "environment",
    "value": {
      "enabled": true,
      "summary": "mz-az-tech-dagobert - Dagobert - CPU Usage",
      "queryDefinition": {
        "type": "METRIC_KEY",
        "metricKey": "builtin:cloud.aws.rds.cpu.usage",
        "aggregation": "AVG",
        "entityFilter": {
          "dimensionKey": "dt.entity.relational_database_service",
          "conditions": [
            {
              "type": "TAG",
              "operator": "EQUALS",
              "value": "[AWS]dynatrace"
            },
            {
              "type": "MANAGEMENT_ZONE",
              "operator": "EQUALS",
              "value": "mz-az-tech-dagobert"
            }
          ]
        },
        "dimensionFilter": []
      },
      "modelProperties": {
        "type": "STATIC_THRESHOLD",
        "threshold": 99,
        "alertOnNoData": false,
        "alertCondition": "ABOVE",
        "violatingSamples": 3,
        "samples": 5,
        "dealertingSamples": 5
      },
      "eventTemplate": {
        "title": "mz-az-tech-dagobert - CPU Usage",
        "description": "CPU Usage Custom Alert",
        "eventType": "CUSTOM_ALERT",
        "davisMerge": false,
        "metadata": []
      },
      "eventEntityDimensionKey": "dt.entity.relational_database_service",
      "legacyId":null
    }
}