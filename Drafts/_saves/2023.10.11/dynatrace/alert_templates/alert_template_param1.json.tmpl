{
    "schemaId": "builtin:anomaly-detection.metric-events",
    "schemaVersion": "1.0.14",
    "scope": "environment",
    "value": {
      "enabled": true,
      "summary": "${MANAGEMENT_ZONE} - ${DASHBOARD_NAME} - ${PARAM1_NAME}",
      "queryDefinition": {
        "type": "METRIC_KEY",
        "metricKey": "${PARAM1_GRAPH}",
        "aggregation": "${PARAM1_AGGREGATION}",
        "entityFilter": {
          "dimensionKey": "dt.entity.relational_database_service",
          "conditions": [
            {
              "type": "TAG",
              "operator": "EQUALS",
              "value": "${DB_TAGS}"
            },
            {
              "type": "MANAGEMENT_ZONE",
              "operator": "EQUALS",
              "value": "${MANAGEMENT_ZONE}"
            }
          ]
        },
        "dimensionFilter": []
      },
      "modelProperties": {
        "type": "STATIC_THRESHOLD",
        "threshold": ${PARAM1_THRESHOLD_RED},
        "alertOnNoData": false,
        "alertCondition": "${PARAM1_CONDITION}",
        "violatingSamples": 3,
        "samples": 5,
        "dealertingSamples": 5
      },
      "eventTemplate": {
        "title": "${MANAGEMENT_ZONE} - ${PARAM1_NAME}",
        "description": "${PARAM1_NAME} Custom Alert",
        "eventType": "CUSTOM_ALERT",
        "davisMerge": false,
        "metadata": []
      },
      "eventEntityDimensionKey": "dt.entity.relational_database_service",
      "legacyId":null
    }
}