{
  "schemaId": "builtin:anomaly-detection.metric-events",
  "schemaVersion": "1.0.14",
  "scope": "environment",
  "value": {
    "enabled": true,
    "summary": "${MANAGEMENT_ZONE} - ${DASHBOARD_NAME} - ${PARAM2_NAME}",
    "queryDefinition": {
      "type": "METRIC_KEY",
      "metricKey": "${PARAM2_GRAPH}",
      "aggregation": "${PARAM2_AGGREGATION}",
      "entityFilter": {
        "dimensionKey": "dt.entity.relational_database_service",
        "conditions": [
          {
            "type": "TAG",
            "operator": "EQUALS",
            "value": "${DB_TAGS}"
          },
          {
            "type": "MANAGEMENT_ZONE",
            "operator": "EQUALS",
            "value": "${MANAGEMENT_ZONE}"
          }
        ]
      },
      "dimensionFilter": []
    },
    "modelProperties": {
      "type": "STATIC_THRESHOLD",
      "threshold": ${PARAM2_THRESHOLD_RED},
      "alertOnNoData": false,
      "alertCondition": "${PARAM2_CONDITION}",
      "violatingSamples": 5,
      "samples": 30,
      "dealertingSamples": 30
    },
    "eventTemplate": {
      "title": "${MANAGEMENT_ZONE} - ${PARAM2_NAME}",
      "description": "${PARAM2_NAME} Custom Alert",
      "eventType": "CUSTOM_ALERT",
      "davisMerge": false,
      "metadata": []
    },
    "eventEntityDimensionKey": "dt.entity.relational_database_service",
    "legacyId":null
  }
}